# === n8n Local Test Environment (.env) ===
# Porta local do n8n
N8N_PORT=5678

# Host/URL públicos (para webhooks e editor). Localmente, deixe http://localhost:5678
N8N_HOST=localhost
N8N_PUBLIC_URL=http://localhost:5678
WEBHOOK_URL=http://localhost:5678

# Fuso horário
TZ=America/Sao_Paulo
GENERIC_TIMEZONE=America/Sao_Paulo

# Segurança — troque este valor se quiser
N8N_ENCRYPTION_KEY=a4fa3c2fd544945b587ff900de013a04769033f585ff671ed08cb8d692a0ad6a

# Execuções (ajustes seguros para testes)
EXECUTIONS_MODE=regular
EXECUTIONS_DATA_SAVE_ON_SUCCESS=none
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_ON_PROGRESS=true
EXECUTIONS_DATA_PRUNE=true
# Max age: 14 dias (336 horas)
EXECUTIONS_DATA_MAX_AGE=336

# Diagnóstico/telemetria
N8N_DIAGNOSTICS_ENABLED=false

# Logs
N8N_LOG_LEVEL=info
